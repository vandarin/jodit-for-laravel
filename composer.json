{
    "name": "do6po/laravel-jodit",
    "description": "An easy way to use the Jodit editor with your Laravel Api service.",
    "type": "library",
    "version": "1.0.4",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "illuminate/http": "^10",
        "illuminate/cache": "^10",
        "illuminate/filesystem": "^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "orchestra/testbench": "^4|^5",
        "mockery/mockery": "^1.0",
        "laravel/framework": "^10",
        "fakerphp/faker": "^1.9.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Boiko Sergii",
            "email": "helvisbox@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Do6po\\LaravelJodit\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Do6po\\LaravelJodit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Do6po\\LaravelJodit\\Providers\\JoditServiceProvider"
            ]
        }
    }
}
